<Window x:Class="_62413___Project.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_62413___Project"
        mc:Ignorable="d"
        FontFamily="Verdana"
        Title="MainWindow" Height="450" Width="800">

    <Window.Resources>
        <local:ThemeSwitch x:Key="ThemeSwitch"/>
    </Window.Resources>

    <Grid Background="{DynamicResource BackgroundBrush}" Margin="4,0,-4,0">
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="Auto"/>
        </Grid.RowDefinitions>
        
        <Frame x:Name="Main" Margin="0,0,0,0" NavigationUIVisibility="Hidden" Grid.Row="1"/>
    </Grid>
</Window>
